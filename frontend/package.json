{
  "name": "datanova-frontend",
  "version": "1.0.0",
  "description": "Frontend for DataNova Data Governance Platform",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "lint:format": "prettier --check .",
    "lint": "echo 'lint via type-check'; npm run type-check",
    "format": "echo 'format placeholder'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:real": "playwright test src/test/e2e/real-backend.spec.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:visual": "percy exec -- playwright test tests/visual/",
    "test:visual:local": "playwright test tests/visual/",
    "test:visual:update": "percy exec -- playwright test tests/visual/ --update-snapshots"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@sentry/vue": "^10.10.0",
    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.14",
    "glob": "^11.0.3",
    "pinia": "^2.3.1",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.2.4",
    "vue3-grid-layout-next": "^1.0.7"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "puppeteer": "^24.20.0",
    "@percy/cli": "^1.31.2",
    "@percy/playwright": "^1.0.9",
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.55.0",
    "@types/jsdom": "^21.1.7",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vitest": "^3.2.4",
    "vue-tsc": "^1.8.8"
  }
}
